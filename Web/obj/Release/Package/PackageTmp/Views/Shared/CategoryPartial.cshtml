@model IEnumerable<Model.EF.Category>

<div class="list-group border-bottom mb-3">
    <div class="list-group-item active fw-bold" aria-current="true">
        Thể Loại
    </div>
    @foreach (var item in Model)
    {
        <a href="@Url.Action("Category", "Book", new { metaTitle = item.MetaTitle })" class="list-group-item">@item.Name</a>
    }
</div>